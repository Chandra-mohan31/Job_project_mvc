@{
}
<p>This is jobSeekers page</p>

@{
    ViewData["Title"] = "View Jobs";
}
<h1>@ViewData["Title"]</h1>
<table class="table">
    <thead>
        <tr>
            <th>Job ID</th>
            <th>Job Name</th>
            <th>Job Description</th>
            <th>Job Employeer Name</th>
            <th>Job Category</th>
        </tr>
    </thead>

    @foreach (var job in (List<Jobs>)ViewData["jobsList"])
    {
        <tr>
            <td>@job.Id</td>
            <td>@job.Name</td>
            <td>@job.Description</td>
            <td>@job.employeerName</td>
            <td>@job.category</td>
        </tr>
    }
</table>